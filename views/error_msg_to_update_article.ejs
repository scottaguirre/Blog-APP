<% include ../partials/header.ejs %>

	<div class="container">
		<div class="row">
			<div class="col-md-6 offset-md-3">
				
				<% if( typeof msg != 'undefined'){ %><div class="alert alert-success 
					alert-dismissible fade show" role="alert"> <%= msg %> 
					<button type="button" class="close" data-dismiss="alert" aria-label="Close"> 
						<span aria-hidden="true">&times;</span>
					</button> <br> <a href=" <%= link %>">Go to article</a> 
				<% } %> 
				
				<% if( typeof msg_signin != 'undefined'){ %><div class="alert alert-success 
					alert-dismissible fade show" role="alert"> <%= msg_signin %> 
					<button type="button" class="close" data-dismiss="alert" aria-label="Close"> 
						<span aria-hidden="true">&times;</span>
					</button> <br> <a href=" <%= link %>">Log In</a> 
				<% } %> 
			
			
				
			</div>		
		</div>	
	</div>

		
		
		
<% include ../partials/footer.ejs %>